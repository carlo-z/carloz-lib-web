#search_panel
{
    margin: 0px 1% 0px 1%;
}

#search_panel .header
{
    margin-bottom: 10px;
}
#search_panel .container
{
}

#search_panel .container .tool_bar{
    padding: 10px 0px 10px 0px;
    border: 1px solid #C0C0C0;
}

#search_panel .container .label_arr_list
{
    margin: 5px 0px 5px 0px;
}

#search_panel .container .label_arr_list .label_arr_item
{
    margin: 5px 0px 5px 0px;
}

#search_panel .container .label_arr_list .label_arr_item h3
{
    font-size: 18px;
    display: inline-block;
    width: auto;
}

#search_panel .container .label_arr_list .label_arr_item li
{
    padding: 0px 10px 0px 10px;
    float: left;
}

#search_panel .container .label_arr_list .label_arr_item a
{
    
}

#search_panel .go-to-top
{
    position: fixed;
    bottom: 100px;
    right: 50px;
    border: 1px solid transparent;
    background-color: transparent;
    border-radius: 50px;
    padding: 7px;
    display: none;
    transition: background-color .5s ease-out, border-color .75s ease-out;
}
#search_panel .go-to-top:hover
{
    background-color: #cfcfcf;
    border-color: #ccc;
}
